{
  "pattern": "**/*.js",
  "options": {
      "UseMinExt": false,
      "MinifyOptions": {
        "compress": {
          "drop_debugger": false,
          "global_defs": {},
          "pure_funcs": []
        },
        "mangle": {
          "reserved": [ "requirejs", "config"],
          "properties": {
            "reserved": ["config", "baseUrl", "paths", "jquery", "lib"]
          }
        },
       "output": {
          "comments": "/^!/",
          "beautify": false,
          "braces": true,
          "quote_style": 2
        },
        "sourceMap": {}
      },
      "OutDir": "build",
      "RootDir": "src",
      "RemoveSource": false,
      "Debug": false
    }
}